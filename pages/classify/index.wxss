page{
  background: #f1f1f1;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #f1f1f1;
  color: black;
}
.nav_left{
  display: inline-block;
  width: 25%;
  height: 100%;
  background: #f1f1f1;
  text-align: center;
}
.nav_left .nav_left_items{
  height: 35px;
  line-height: 35px;
  padding: 5px 0;
  border-bottom: 1px solid #f8f8f8;
  border-right: 1px solid #f8f8f8;
  font-size: 14px;
  background: #fff;
}
.nav_left .nav_left_items.active{
  background: #f1f1f1;
  color: #f9752b;
  border-right: none;
}
.nav_right{
  position: absolute;
  top: 0;
  right: 0;
  width: 69%;
  margin: 12px 10px 20px 10px;
  background: #fff;
  box-sizing: border-box;
  /*border-radius: 4px;*/
}
.nav_right .nodata{
  text-align: center;
  margin: 10px;
  font-size: 14px;
}
.nav_right .nav_right_items{
  float: left;
  width: 33%;
  height: 85px;
  text-align: center;
  padding-top: 20px;
}
.nav_right .nav_right_items image{
  width: 50px;
  height: 45px;
}
.nav_right .nav_right_items text{
  color: #6f6f6f;
  display: block;
  margin-top: 10px;
  font-size: 12px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}